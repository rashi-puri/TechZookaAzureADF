{
	"name": "conquerordataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable",
						"type": "DatasetReference"
					},
					"name": "sqldbsource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqloutput",
						"type": "DatasetReference"
					},
					"name": "AnalyticsLayer"
				}
			],
			"transformations": [
				{
					"name": "AggOnTransactionMode"
				},
				{
					"name": "AlterRow1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CustomerIdentifier as string,",
				"          gender as string,",
				"          SeniorCitizen as boolean,",
				"          Partner as boolean,",
				"          Dependents as boolean,",
				"          Country as string,",
				"          Age as integer,",
				"          Country2 as string,",
				"          PremiumCustomer as boolean,",
				"          PrefferedTransactionMethod as string,",
				"          Total_Asset as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sqldbsource",
				"derivedColumn1 aggregate(groupBy(PrefferedTransactionMethod),",
				"     Total_Count_Transaction = countDistinct(CustomerIdentifier),",
				"          Total_Asset = sum(Total_Asset)) ~> AggOnTransactionMode",
				"AggOnTransactionMode alterRow(upsertIf(true())) ~> AlterRow1",
				"sqldbsource derive(Total_Asset = ERROR_FUNCTION('')) ~> derivedColumn1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['TransactionMethod'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TransactionMethod = PrefferedTransactionMethod,",
				"          Total_Count_Transaction,",
				"          Total_Asset",
				"     )) ~> AnalyticsLayer"
			]
		}
	}
}