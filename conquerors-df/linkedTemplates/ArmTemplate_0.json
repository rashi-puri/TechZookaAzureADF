		{
			"name": "[concat(parameters('factoryName'), '/Shanghai')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "joining two tables to add country column to the Shanghai table.",
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "universityCountry",
								"type": "DatasetReference"
							},
							"name": "UniversityCountry"
						},
						{
							"dataset": {
								"referenceName": "DelimitedText3",
								"type": "DatasetReference"
							},
							"name": "Shanghai"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "AzureSqlTable2",
								"type": "DatasetReference"
							},
							"name": "updatedShanghaiTable",
							"description": "adding the updated table in sql databse"
						}
					],
					"transformations": [
						{
							"name": "join1"
						},
						{
							"name": "updatedShanghai",
							"description": "deleted school column and arranged the other columns"
						},
						{
							"name": "alterRow1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          school_name as string,",
						"          country as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> UniversityCountry",
						"source(output(",
						"          world_rank as string,",
						"          university_name as string,",
						"          national_rank as string,",
						"          total_score as double,",
						"          alumni as double,",
						"          award as double,",
						"          hici as double,",
						"          ns as double,",
						"          pub as double,",
						"          pcp as double,",
						"          year as short",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> Shanghai",
						"UniversityCountry, Shanghai join(school_name == university_name,",
						"     joinType:'right',",
						"     matchType:'exact',",
						"     ignoreSpaces: false,",
						"     broadcast: 'auto')~> join1",
						"join1 select(mapColumn(",
						"          world_rank,",
						"          university_name,",
						"          country,",
						"          national_rank,",
						"          total_score,",
						"          alumni,",
						"          award,",
						"          hici,",
						"          ns,",
						"          pub,",
						"          pcp,",
						"          year",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> updatedShanghai",
						"updatedShanghai alterRow(upsertIf(true())) ~> alterRow1",
						"alterRow1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     deletable:false,",
						"     insertable:false,",
						"     updateable:false,",
						"     upsertable:true,",
						"     keys:['university_name'],",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError') ~> updatedShanghaiTable"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/universityCountry')]",
				"[concat(variables('factoryId'), '/datasets/DelimitedText3')]",
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable2')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/conquerordataflow')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "AzureSqlTable",
								"type": "DatasetReference"
							},
							"name": "sqldbsource"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "AzureSqloutput",
								"type": "DatasetReference"
							},
							"name": "AnalyticsLayer"
						}
					],
					"transformations": [
						{
							"name": "AggOnTransactionMode"
						},
						{
							"name": "AlterRow1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          CustomerIdentifier as string,",
						"          gender as string,",
						"          SeniorCitizen as boolean,",
						"          Partner as boolean,",
						"          Dependents as boolean,",
						"          Country as string,",
						"          Age as integer,",
						"          Country2 as string,",
						"          PremiumCustomer as boolean,",
						"          PrefferedTransactionMethod as string,",
						"          Total_Asset as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> sqldbsource",
						"sqldbsource aggregate(groupBy(PrefferedTransactionMethod),",
						"     Total_Count_Transaction = countDistinct(CustomerIdentifier),",
						"          Total_Asset = sum(Total_Asset)) ~> AggOnTransactionMode",
						"AggOnTransactionMode alterRow(upsertIf(true())) ~> AlterRow1",
						"AlterRow1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:true,",
						"     upsertable:false,",
						"     keys:['TransactionMethod'],",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          TransactionMethod = PrefferedTransactionMethod,",
						"          Total_Count_Transaction,",
						"          Total_Asset",
						"     )) ~> AnalyticsLayer"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable')]",
				"[concat(variables('factoryId'), '/datasets/AzureSqloutput')]"
			]
		},
